@model IEnumerable<HospitalASP.PatientCard>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CardCreatingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CardOwnerFIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Historyilling.AnalysisResult)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CardCreatingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardOwnerFIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Historyilling.AnalysisResult)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PatientCardNum }) |
            @Html.ActionLink("Details", "Details", new { id=item.PatientCardNum }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PatientCardNum })
        </td>
    </tr>
}

</table>
